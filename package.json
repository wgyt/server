{
	"name": "@onedotprojects/state",
	"version": "1.0.0",
	"description": "Scratch online detction server",
	"main": "index.js",
	"scripts": {
		"build": "npx tailwindcss-cli@latest build -o tailwind.css",
		"start": "node --trace-warnings ."
	},
	"keywords": [
		"scratch",
		"online",
		"detection"
	],
	"author": "William Horning and Contributors",
	"license": "MIT",
	"dependencies": {
		"cors": "^2.8.5",
		"express": "^4.17.1",
		"jsoning-no-limits": "^0.11.21",
		"node-fetch": "^2.6.1",
		"retronid": "^1.0.2"
	},
	"devDependencies": {
		"@types/node": "^14.14.37",
		"prettier": "^2.2.1"
	},
	"bugs": {
		"url": "https://github.com/onedotprojects/onestate-server/issues/new",
		"email": "wgyt@wgyt.tk"
	},
	"homepage": "https://github.com/onedotprojects/onestate-server",
	"private": true,
	"engines": {
		"node": "*"
	},
	"jshintConfig": {
		"maxerr": 50,
		"bitwise": true,
		"camelcase": true,
		"curly": true,
		"eqeqeq": true,
		"forin": true,
		"freeze": true,
		"immed": true,
		"latedef": true,
		"newcap": false,
		"noarg": true,
		"noempty": true,
		"nonbsp": true,
		"nonew": false,
		"plusplus": false,
		"quotmark": true,
		"undef": true,
		"unused": true,
		"strict": false,
		"maxparams": false,
		"maxdepth": 5,
		"maxstatements": false,
		"maxcomplexity": 30,
		"maxlen": 120,
		"varstmt": false,
		"asi": false,
		"boss": false,
		"debug": false,
		"eqnull": false,
		"esversion": 9,
		"moz": false,
		"evil": false,
		"expr": true,
		"funcscope": true,
		"globalstrict": false,
		"iterator": true,
		"lastsemic": false,
		"laxbreak": false,
		"laxcomma": true,
		"loopfunc": false,
		"multistr": true,
		"noyield": false,
		"notypeof": true,
		"proto": true,
		"scripturl": true,
		"shadow": false,
		"sub": false,
		"supernew": true,
		"node": true
	},
	"prettier": {
		"tabWidth": 4,
		"useTabs": true,
		"trailingComma": "all",
		"jsxBracketSameLine": true,
		"vueIndentScriptAndStyle": true,
		"embeddedLanguageFormatting": "auto",
		"printWidth": 120
	}
}
